<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/app_bar_tracker_dashboard_with_menu"
        tools:context=".TrackerDashboardWithMenu">


    <fragment
            android:id="@+id/activity_place_map"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="match_parent"

            android:layout_height="match_parent"
            tools:context=".TrackerDashboardWithMenu"/>


    <TextView android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/lblEsti"
              app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="8dp"
              android:gravity="center_horizontal" android:textColor="@color/colorAccent"
              android:textSize="21sp"
              app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
              app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>


</android.support.constraint.ConstraintLayout>